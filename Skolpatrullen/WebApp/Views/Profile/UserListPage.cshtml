@model System.Collections.Generic.List<Database.Models.User>

@{
    ViewData["Title"] = "Alla användare";
}
<a href="##" onClick="history.go(-1); return false;">Gå tillbaka</a>
<h1>Alla användare</h1>

@foreach (var item in Model)
{
    <div class="row">
        <div class="col-lg-6">
            <div class="alert-secondary font-weight-bold box-shadow"><i class="fas fa-user" style="margin-left:5px margin-right:5px"></i>
                @Html.DisplayFor(modelItem => item.FirstName) @Html.DisplayFor(modelItem => item.LastNames) 
            <a class="text-dark float-right font-weight-normal" asp-area="" asp-controller="Profile" asp-action="AdminChangeProfile" asp-route-Id="@item.Id">
                <i class="fas fa-edit" style="margin-left:5px"></i>Ändra
                </a>
            </div>
            <div class="bg-light box-shadow"> @Html.DisplayFor(modelItem => item.SocialSecurityNr), SuperUser: @Html.DisplayFor(modelItem => item.IsSuperUser)</div>
        </div>
    </div>
    <br />
}

